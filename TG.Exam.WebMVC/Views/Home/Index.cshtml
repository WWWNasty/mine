@model List<TG.Exam.WebMVC.Models.User>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <table class="table">
      <thead>
        <tr>
          <th>#</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Age</th>
          <th>Fetch method</th>
        </tr>
      </thead>
      <tbody>
      @for (int i = 0; i < Model.Count; i++)
      {
        <tr>
          <th scope="row">@(i+1)</th>
          <td>@Model[i].FirstName</td>
          <td>@Model[i].LastName</td>
          <td>@Model[i].Age</td>
          <td>Sync</td>
        </tr>
      }
      </tbody>
    </table>
</div>